{
    "pipeline": {
      "name": "LogD-ingestion"
    },
    "transform": {
      "image": "pharmbio/cpsign-database-ingestion",
      "cmd": [ "/bin/bash" ],
      "stdin": [
        "python3 data_ingestion.py"
      ]
    },
    "standby": true,
    "resource_limits": {
      "memory": "4G",
      "cpu": 2
    },
    "resource_requests": {
        "memory" : "2G",
        "cpu": 1
    },
    "input" : {
      "atom": {
        "repo": "config",
        "glob": "/"
      }
    }
  }