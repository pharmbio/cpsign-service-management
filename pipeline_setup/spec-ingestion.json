{
    "pipeline": {
      "name": "LogD-ingestion"
    },
    "transform": {
      "image": "pharmbio/cpsign-database-ingestion:cps-0.6.16",
      "cmd": [ "/bin/bash" ],
      "stdin": [
        "python3 data_ingestion.py"
      ],
      "secrets": [
        {
          "name": "logd-chembl-mysql",
          "mount_path": "/etc/creds/"
        }
      ]
    },
    "standby": true,
    "resource_limits": {
      "memory": "4G",
      "cpu": 2
    },
    "resource_requests": {
        "memory" : "2G",
        "cpu": 1
    },
    "input" : {
      "atom": {
        "repo": "config",
        "glob": "/"
      }
    }
  }