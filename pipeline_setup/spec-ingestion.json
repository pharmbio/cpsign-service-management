{
    "input": {
        "atom": {
            "glob": "/",
            "repo": "config"
        }
    },
    "pipeline": {
        "name": "LogD-ingestion"
    },
    "resource_limits": {
        "cpu": 2,
        "memory": "4G"
    },
    "resource_requests": {
        "cpu": 1,
        "memory": "2G"
    },
    "standby": true,
    "transform": {
        "cmd": [
            "/bin/bash"
        ],
        "image": "pharmbio/cpsign-database-ingestion:cps-0.6.16",
        "secrets": [
            {
                "mount_path": "/etc/creds/",
                "name": "logd-chembl-mysql"
            }
        ],
        "stdin": [
            "python3 data_ingestion.py"
        ]
    }
}
