{
    "input": {
        "atom": {
            "glob": "/",
            "repo": "hERG_config"
        }
    },
    "pipeline": {
        "name": "hERG-ingestion"
    },
    "resource_limits": {
        "cpu": 2,
        "memory": "8G"
    },
    "resource_requests": {
        "cpu": 1,
        "memory": "2G"
    },
    "standby": false,
    "transform": {
        "cmd": [
            "/bin/bash"
        ],
        "image": "pharmbio/cpsign-database-ingestion:herg_test5",
        "secrets": [
            {
                "mount_path": "/etc/creds/",
                "name": "chembl-mysql"
            }
        ],
        "stdin": [
            "python3 data_ingestion.py"
        ]
    }
}
