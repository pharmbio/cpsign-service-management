{
    "input": {
        "atom": {
            "glob": "/",
            "repo": "hERG-config"
        }
    },
    "pipeline": {
        "name": "hERG-ingestion"
    },
    "resource_limits": {
        "cpu": 4,
        "memory": "8G"
    },
    "resource_requests": {
        "cpu": 1,
        "memory": "1G"
    },
    "standby": false,
    "transform": {
        "cmd": [
            "/bin/bash"
        ],
        "image": "pharmbio/cpsign-database-ingestion:herg_test13",
        "secrets": [
            {
                "mount_path": "/etc/creds/",
                "name": "chembl-mysql"
            }
        ],
        "stdin": [
            "python3 data_preparation.py",
            "python3 parameter_file_preparation.py"
        ]
    }
}
